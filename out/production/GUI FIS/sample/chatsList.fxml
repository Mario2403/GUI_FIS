<?import javafx.scene.text.*?>

<BorderPane prefHeight="597.0" prefWidth="438.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="sample.Controller">
  <bottom>
    <HBox alignment="BOTTOM_RIGHT" prefHeight="34.0" prefWidth="439.0" BorderPane.alignment="BOTTOM_RIGHT">
      <children>
        <Button fx:id="bVolver" mnemonicParsing="false" onAction="#changeToPrincipal" prefHeight="34.0" prefWidth="75.0" text="Volver" textOverrun="ELLIPSIS">
          <font>
            <Font size="15.0" />
          </font>
        </Button>
      </children>
      <BorderPane.margin>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
      </BorderPane.margin>
    </HBox>
  </bottom>
  <center>
    <VBox onMouseClicked="#refreshList" prefHeight="200.0" prefWidth="100.0">
      <children>
        <HBox alignment="CENTER_LEFT" onMouseClicked="#changeToSingleChat" prefHeight="100.0" prefWidth="200.0">
          <children>
            <ImageView fx:id="fotoLista" fitHeight="100.0" fitWidth="133.33333333333331" pickOnBounds="true" preserveRatio="true" />
            <Label fx:id="aliasLista" alignment="CENTER" contentDisplay="CENTER" prefWidth="313.0" text="">
              <font>
                <Font size="25.0" fx:id="x1" />
              </font>
            </Label>
          </children>
        </HBox>
      </children>
    </VBox>
  </center>
  <top>
    <MenuBar>
      <menus>
        <Menu mnemonicParsing="false" text="File">
          <items>
            <MenuItem mnemonicParsing="false" onAction="#exit" text="Close" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
          <items>
            <MenuItem mnemonicParsing="false" onAction="#showAboutUs" text="About" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
  </top>
</BorderPane>
